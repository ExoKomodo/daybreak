[alias]
        co = checkout
        cob = checkout -b
        cp = cherry-pick
        com = commit -m
        st = status
        pushup = !git push --set-upstream origin `git rev-parse --abbrev-ref HEAD`
        br = branch
        hist = log --pretty=format:'%h %ad | %s%d [%an]' --graph --date=short
        type = cat-file -t
        dump = cat-file -p
        rebi = rebase -i
        del = branch -D
[branch]
        autosetuprebase = always
[core]
        editor = vim
[pull]
        rebase = true